{
  "name": "winprobviz",
  "version": "1.0.0",
  "description": "Win probability visualizer",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project .",
    "webpack-json": "webpack --config app/webpack.prod.js --json > webpack.json",
    "build": "tsc --project app && tsc --project . && webpack --config app/webpack.dev.js",
    "build-prod": "tsc --project app && tsc --project . && webpack --config app/webpack.prod.js",
    "scrape": "node -r ts-node/register --inspect-brk=9229 scripts/scrape.ts",
    "serve-dev": "node -r ts-node/register --inspect-brk=9229 index.ts",
    "serve": "forever start index.js",
    "stop": "forever stopall",
    "replicate": "node -r ts-node/register --inspect-brk=9229 scripts/replicate.ts"
  },
  "author": "Kenny Jackelen",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.4.0",
    "bezier-easing": "^2.0.3",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "material-ui": "^0.20.0",
    "mlbstatsapi": "git+https://github.com/kennyjackelen/mlbstatsapi.git",
    "pouchdb": "^6.4.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@types/bezier-easing": "^2.0.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.11.1",
    "@types/material-ui": "^0.21.1",
    "@types/pouchdb": "^6.3.2",
    "@types/react": "^16.3.9",
    "@types/react-dom": "^16.0.5",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-node": "^5.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.2"
  }
}
